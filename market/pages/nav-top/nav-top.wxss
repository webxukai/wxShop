/* pages/nav-top/nav-top.wxss */
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  /* background: #fff; */
  color: #fff;
}
/* 标题要居中 */
.nav-title {
  position: absolute;
  text-align: center;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 37rpx;
  color: #fff;
  z-index:9;
}

.nav-capsule {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  width: 140rpx;
  height: 100%;
}
.back-img{
  width:35rpx;
  height:35rpx;
}
.back-text{
  bottom:3rpx;
  position:relative;
  margin-left:4rpx;
  font-size:32rpx;
}

.back-pre, .back-home {
  width: 32rpx;
  height: 36rpx;
  margin-top: 4rpx;
  padding: 10rpx;
}
.nav-capsule .back-home {
  width: 36rpx;
  height: 40rpx;
  margin-top: 3rpx;
}
  
.navigation-bg1-img{
  width: 100%;
  position:absolute;
  top:0;
  left:0;
  z-index:-1;
}
.navigation-bg2-img{
  width: 100%;
  height: 476rpx;
  position:absolute;
  top:0;
  left:0;
  z-index:-1;
}