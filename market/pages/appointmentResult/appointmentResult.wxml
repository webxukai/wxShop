<!--pages/appointmentResult/appointmentResult.wxml-->
<view class='index-title-wrapper' style="{{height}}">
  <nav-top navbarData='{{navbarData}}'></nav-top>
</view>

<view class="container">
<image class='fill-img' src='../../img/Fill .png'></image>
  <view class='fill-title'>恭喜你兑换成功!</view>
    <view  class='success-info-wrapper'>
      <view class='text-align-left'>
        <text>收货人 : </text>
        <text>{{ name}}</text>
      </view>
      <view class='text-align-left'>
        <text>地址 : </text>
        <text>{{address}}</text>
      </view>
      <view class='text-align-left'>
        <text>手机号 : </text>
        <text>{{mobile}}</text>
      </view>
      <view class='text-align-left'>
        <text>{{delivery_name}} : </text>
        <text>{{delivery_no}}</text>
      </view>
    </view>
    <view >
      <button class='fill-btn' bindtap='backToIndex' >完成</button>
    </view>
</view>